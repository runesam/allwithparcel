{
  "name": "allwithparcel",
  "version": "1.0.0",
  "description": "trying parcel for the first time with react app :)",
  "main": "index.js",
  "scripts": {
    "build-css": "node-sass-chokidar app/ -o app/",
    "watch-css": "npm run build-css && node-sass-chokidar app/ -o app/ --watch --recursive",
    "start:dev": "cross-env NODE_ENV=development parcel index.html -p 3000",
    "start": "npm-run-all -p watch-css start:dev",
    "build:prod": "cross-env NODE_ENV=production parcel build index.html --no-minify --public-url /",
    "build": "npm-run-all build-css build:prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/runesam/allwithparcel.git"
  },
  "keywords": [
    "MiT"
  ],
  "author": "Sam Ewdala",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/runesam/allwithparcel/issues"
  },
  "homepage": "https://github.com/runesam/allwithparcel#readme",
  "dependencies": {
    "autoprefixer": "^7.2.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "node-sass": "^4.7.2",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "postcss-modules": "^1.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "cross-env": "^5.1.3",
    "parcel-bundler": "^1.3.1"
  }
}
